pipeline{
    agent none
    stages{
        stage ('parallel build') {
            parallel{
                stage ('biuld-1'){
                    agent{label 'node1'}
                    steps{
                        echo "this is parallel build-1"
                    }
                }
                stage ('biuld-2'){
                    agent{label 'node2'}
                    steps{
                        echo "this is parallel build-2"
                    }
                }
            }

        }
  }
}
